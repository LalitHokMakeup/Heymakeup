:root {
  --main-padding: 0 6.25vw;
  --main-margin: 0 6.25vw;
  --announcement_bg: #E33F75;
  --white: #ffffff;
  --dark: #393939;
  --pink: #E33F75;
  --font-light: 400;
  --font-medium: 500;
  --font-bold: 600;
  --font-extra-bold: 700;
  --font-montserrat: 'Montserrat', sans-serif;
  --font-poppins: 'Poppins', sans-serif;
  --bg-topbrands: #FFE1EC;
}
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  font-family: var(--font-poppins);
}
ul {
  list-style: none;
}
a {
  text-decoration: none !important;
}
.asp34 {
  aspect-ratio: 3 / 4 !important;
}
.main-spaces {
  padding: 50px 120px;
  overflow: hidden;
}
.swatch input:checked + label {
  outline: 1px solid #cf8c6a;
  outline-offset: 2px;
}
.right-menu-list {
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  padding: 0;
  margin-bottom: 0;
}
.right-menu-list li a {
  text-decoration: none;
  font-weight: bold;
  color: #000;
  line-height: 24px;
}
/* .slideshow {
  position: relative;
  top: -40px;
  max-width: 100%;
  margin: 0 auto;
} */
.menus {
  position: absolute;
  top: 0;
  z-index: 1;
  padding: 0 1rem;
  overflow-x: scroll;
  width: 100%;
  height: 56px;
}
.menus::-webkit-scrollbar {
  display: none;
}
.menus ul {
  gap: 24px;
  height: 100%;
  justify-content: start;
  white-space: nowrap;
  padding: 0;
}
.jdgm-temp-hidden {
  display: none !important;
}
.list-inline-item a {
  text-decoration: none;
  color: #fff;
  font-size: 12px;
  text-transform: uppercase;
  font-weight: var(--font-medium);
}

#sliderItem_One li.prev {
  left: 90%;
}
#sliderItem_One li.next {
  right: 90%;
}

.my-slider {
  height: auto;
}
.my-slider img {
  height: 100%;
  width: 100%;
}
section.banner-container {
  position: relative;
  top: -4rem;
}
#judgeme_product_reviews {
  margin: 0 10%;
}
@media only screen and (max-width: 768px) {
  .slideshow {
    top: 0 !important;
  }
  section.banner-container {
    top: -2rem;
  }
  #judgeme_product_reviews {
    margin: 0 3% !important;
  }
}
.fa-solid {
  font-size: 27px;
}

.d_cent {
  display: flex;
  justify-content: center;
  align-items: center;
}
.offcanvas-collapse {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 100%;
  width: 96%;
  padding-right: 1rem;
  padding-left: 1rem;
  overflow-y: auto;
  visibility: hidden;
  background-color: #fff;
  transition: visibility 0.3s ease-in-out, -webkit-transform 0.3s ease-in-out;
  transition: transform 0.3s ease-in-out, visibility 0.3s ease-in-out;
  transition: transform 0.3s ease-in-out, visibility 0.3s ease-in-out, -webkit-transform 0.3s ease-in-out;
  z-index: 1;
}
.deals_txt {
  font-size: 28px;
  letter-spacing: 1px;
  font-weight: var(--font-medium);
  font-family: var(--font-poppins);
  text-transform: capitalize;
  color: #191919;
  text-align: center !important;
}
.grid_center {
  display: grid;
  place-content: center;
}

/* Toaster */
.toastersd {
  position: fixed;
  bottom: 40px;
  z-index: 16000161 !important;
  width: 100%;
}
.notification {
  background-color: #fff;
  padding: 20px;
  width: 350px;
  border-radius: 10px;
  box-shadow: 0 2px 5px #00000033;
  cursor: pointer;
  font-weight: 500;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
  overflow: hidden;
}

.notification i {
  color: #fff;
  border-radius: 50%;
  font-size: 12px;
  width: 24px;
  height: 24px;
  display: flex !important;
  justify-content: center;
  align-items: center;
}

.notification span {
  font-size: 15px;
  color: #191919;
  line-height: 1.6;
  white-space: nowrap;
  gap: 10px;
}
.notification span.view {
  color: #ff669e !important;
  font-weight: 600;
}
.notification.success i {
  background-color: #3fbd637c;
}

@media screen and (max-width:768px) {
  .toastersd {
    bottom: 40px;
  }
  .notification {
    width: 90% !important;
  }
  #insta-feed h2 {
    font-size: 20px !important;
  }
}
/* Toaster Ends */
/* Back to Top btn starts  */
#btn_backToTop {
  background: #e2e3ff;
  width: 50px;
  height: 50px;
  position: fixed;
  bottom: 10%;
  right: 30px;
  transition: all ease-in-out 0.2s;
  opacity: 0;
  visibility: hidden;
  z-index: 1;
}
#btn_backToTop::after {
  content: '\f077';
  font-family: FontAwesome;
  font-size: 1.5rem;
  line-height: 50px;
  color: inherit;
}
#btn_backToTop:hover {
  cursor: pointer;
  background-color: #333;
  color: #e2e3ff;
}
#btn_backToTop:active {
  background-color: #555;
}
#btn_backToTop.show {
  opacity: 1;
  visibility: visible;
}
@media screen and (max-width:768px) {
  #btn_backToTop {
    width: 40px;
    height: 40px;
  }
  #btn_backToTop:after {
    content: "\f077";
    font-family: FontAwesome;
    font-size: 16px;
  }
}
/* Back to Top btn Ends  */
/* Slick Slider  */
.slick-track {
  display: flex;
}
#pv-slider,
#pv-thumb-slider {
  overflow: hidden;
}
.slick-arrow {
  position: absolute;
  top: 50%;
  background: transparent;
  z-index: 1;
}
.slick-arrow.slick-disabled {
  opacity: 0.6;
}
.slick-prev {
  left: 0 !important;
}
.slick-next {
  right: 0 !important;
}
#pv-slider .thumb-slide.slick-slide {
  padding: 5px 0;
}
/* Slick Slider ends */
.wishlist-Icon.d_cent .heart_icon.filled,
.headwishIcon.filled {
  fill: #E0E2FF;
  animation: slowly 0.4s ease;
}
.wishlist-Icon.d_cent .heart_icon.filled path {
  stroke: #E0E2FF;
}
.controls li.next.d_cent,
.controls li.prev.d_cent {
  position: absolute;
  top: 50%;
}
.controls li.next.d_cent {
  left: -20px;
}
.controls li.prev.d_cent {
  right: -20px;
}
.next[aria-disabled=true],
.prev[aria-disabled=true] {
  opacity: 0.5;
}
@media screen and (max-width: 991px) {
  .desktop-screen {
    display: none !important;
  }
  .mobile-screen {
    display: block !important;
  }
}
@media(min-width: 992px) {
  .desktop-screen {
    display: flex;
  }
  .mobile-screen {
    display: none;
  }
  .offcanvas-collapse {
    position: static;
    top: auto;
    bottom: auto;
    left: auto;
    width: auto;
    padding-right: 0;
    padding-left: 0;
    background-color: transparent;
    overflow-y: visible;
    visibility: visible;
  }
}
@media screen and (max-width: 767px) {
  ul.right-menu-list {
    padding: 16px 0;
    text-align: right;
  }
  .deals_txt {
    font-size: 20px !important;
  }
  .main-spaces {
    padding: 48px 12px;
  }
}
@keyframes slowly {
  0% {
    transform: scale(0.8);
  }

  50% {
    transform: scale(1.15);
  }

  100% {
    transform: scale(1);
  }
}
.product_slider_head {
  margin-top: 40px;
}
.product-card {
  max-width: 320px;
  border-radius: 4px;
  overflow: hidden;
  margin: 10px 0;
}

.product-card .product-image {
  position: relative;
}

.tags {
  position: absolute;
  top: 6px;
  left: 6px;
}

.tags .tag {
  width: max-content;
  padding: 3.5px 8px;
  border-radius: 2px;
  display: flex;
  align-items: center;
  justify-content: start;
  font-size: 12px;
  line-height: 14px;
  color: #393939;
  font-style: normal;
  font-weight: 500;
}

.product-card .product-image .exclusive {
  position: absolute;
  top: 9px;
  right: 9px;
}

.product-card .product-image .product-img-1,
.product-card .product-image .product-img-2 {
  width: 100%;
  transition: all 0.2s ease-in-out;
  aspect-ratio: 3 / 4;
}

.product-card .product-image .product-img-2 {
  display: none;
}

.product-card .product-image:hover .product-img-1 {
  display: none;
}

.product-card .product-image:hover .product-img-2 {
  display: block;
}

.product-card .product-image .fav {
  position: absolute;
  right: 10px;
  bottom: 10px;
}

.product-card .product-details {
  margin-bottom: 10px;
  margin-top: 5px;
}

.product-card .product-details .product-brand {
  color: #7e7e7e;
  font-size: 12px;
  font-style: normal;
  font-weight: 400;
  line-height: 14px;
  text-transform: capitalize;
}

.product-card .product-details .product-title {
  color: #393939;
  font-size: 16px;
  font-style: normal;
  font-weight: 400;
  line-height: 24px;
  height: 48px;
  text-transform: uppercase;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  cursor: pointer;
}

.product-card .product-details .rating {
  display: flex;
  gap: 4px;
  height: 14px;
  margin-top: 10px;
}

.product-card .product-details .product-price {
  color: #393939;
  font-size: 14px;
  height: 20px;
  font-style: normal;
  font-weight: 500;
  line-height: 20px;
  display: flex;
  gap: 4px;
  margin-top: 16px;
}

.product-card .product-details .product-price .old-price {
  color: silver;
  font-weight: 400;
  text-decoration: line-through;
}

.product-card .product-details .product-price .discount {
  color: #e33f75;
  font-size: 16px;
  font-weight: 600;
  /* margin-left: 10px; */
}

.product-card .product-details .shades {
  display: flex;
  gap: 4px;
  margin-top: 16px;
  height: 22px;
}

.product-card .product-details .shades .shade {
  width: 20px;
  height: 20px;
  border-radius: 100%;
}

.product-card .select-shade {
  display: flex;
  padding: 12px !important;
  justify-content: center;
  align-items: flex-start;
  gap: 10px;
  color: #404184;
  font-size: 15px;
  font-style: normal;
  font-weight: 500;
  line-height: 22px;
  border-radius: 50px;
  border: 1.5px solid #b1b3e3;
  transition: all 0.2s ease-in-out;
  margin-top: 16px;
  width: 100%;
  background: #fff;
}

.product-card:hover .select-shade {
  background-color: #e1e2fc;
  border: 1.5px solid #e1e2fc;
}

@media screen and (max-width: 576px) {
  .product-card {
    max-width: 180px;
  }

  .product-card .product-image .tags .tag {
    font-size: 10px;
    line-height: 12px;
  }

  .product-card .product-details {
    margin-bottom: 24px;
  }

  .product-card .product-details .product-brand {
    font-size: 11px;
  }

  .product-card .product-details .product-title {
    font-size: 12px;
    line-height: 16px;
    height: 30px;
  }

  .product-card .product-details .rating {
    height: 10px;
    margin-top: 8px;
  }

  .product-card .product-details .product-price {
    font-size: 12px;
    height: 14px;
    line-height: 14px;
  }

  .product-card .product-details .product-price .discount {
    font-size: 12px;
  }

  .product-card .product-details .shades {
    margin-top: 8px;
    height: 22px;
    display: flex;
    align-items: center;
  }

  .product-card .select-shade {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 8px;
    font-size: 12px;
    line-height: 14px;
    /* margin-top: 24px; */
  }
}
/* Quick View Slide  */
.quick-view-slide-container {
  position: fixed;
