  right: 0;
  z-index: 3;
  width: 100vw;
  max-width: 500px;
  background-color: #FFF;
  transform: translateX(100%);
  transition: all ease-in-out 0.3s;
}
.quick-view-slide-open {
  transform: translate(0, 0);
}

.quickview-overlay,
.overlay-container {
  position: fixed;
  height: 100vh;
  width: 100vw;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 2;
  display: none;
  background-color: rgba(0, 0, 0, 0.5);
}
.quickview-overlay-open,
.overlay-open {
  display: block;
}


