<script>
  const megamenuSubitems = document.querySelectorAll('.megamenu-subitems');
  const megamenuSublinks = document.querySelectorAll('.sub-links');
  megamenuSubitems.forEach((item) => {
    item.addEventListener('mouseenter', () => {
      megamenuSublinks.forEach((links) => {
        const Tabs = links.getAttribute('data-toggle');
        item.id == Tabs ? links.classList.add('sublink-active') : links.classList.remove('sublink-active');
      });
    });
  });
</script>

